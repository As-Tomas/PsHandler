                        <Label VerticalContentAlignment="Center" Padding="2" Foreground="Blue" Content="dolor" />
                        <Label VerticalContentAlignment="Center" Padding="2" Foreground="Black" Content="sit" />
                        <Label VerticalContentAlignment="Center" Padding="2" Foreground="Black" Content="amet" />
                        <Label VerticalContentAlignment="Center" Padding="0" Foreground="Black" Content="," />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal">
                        <Label VerticalContentAlignment="Center" Padding="2" Foreground="Black" Content="consectetur" />
                        <Label VerticalContentAlignment="Center" Padding="2" Foreground="Black" Content="adipisicing" />
                        <Label VerticalContentAlignment="Center" Padding="2" Foreground="Black" Content="elit" />
                        <Label VerticalContentAlignment="Center" Padding="0" Foreground="Black" Content="&quot;" />
                    </StackPanel>
                </StackPanel>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label VerticalContentAlignment="Center" Padding="2" Foreground="Black" Content="Will this text pass the filter?" FontWeight="SemiBold" />
                <Image HorizontalAlignment="Center" VerticalAlignment="Center" Height="16" Width="16" Margin="5,0,0,0" Source="../../Images/delete.png" />
