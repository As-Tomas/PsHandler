                    <Label VerticalContentAlignment="Center" Padding="2" Foreground="Black" Content="Filter:" FontWeight="SemiBold" />
                    <Label VerticalContentAlignment="Center" Padding="2" Foreground="Blue" Content="ipsum dolor" />
                </StackPanel>
